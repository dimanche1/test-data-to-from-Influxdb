            <stringProp name="script">//def remote_hosts = [&quot;load_gen1&quot;,&quot;load_gen2&quot;,&quot;load_gen3&quot;,&quot;load_gen4&quot;]
def remote_hosts = &quot;${__property(remote_hosts)}&quot;
remote_hosts = remote_hosts.split(&quot;,&quot;)
if(vars.get(&quot;count&quot;) == null || vars.get(&quot;count&quot;).toInteger() &gt;= remote_hosts.size()) vars.put(&quot;count&quot;, &quot;0&quot;) 
vars.put(&quot;load_gen&quot;, remote_hosts.getAt(vars.get(&quot;count&quot;).toInteger()))</stringProp>
